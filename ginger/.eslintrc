{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "no-console": "warn",
    "quotes": ["warn", "single"],
    "prettier/prettier": "warn",
    "no-unused-vars": "warn"
