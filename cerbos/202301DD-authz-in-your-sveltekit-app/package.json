{
  "name": "authz-with-cerbos-for-sveltekit-app",
  "description": "A demo of Cerbos for AuthZ in SvelteKit",
  "keywords": [
    "cerbos",
    "svelte-kit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thisdot/blog-demos/tree/main/cerbos/202301DD-authz-in-your-sveltekit-app"
  },
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "cerbos": "docker run --rm --name cerbos -it -v $(pwd)/cerbos/policies:/policies -p 3592:3592 -p 3593:3593  ghcr.io/cerbos/cerbos:0.22.0",
    "cerbos:win": "docker run --rm --name cerbos -it -v  %cd%/cerbos/policies:/policies -p 3592:3592 -p 3593:3593  ghcr.io/cerbos/cerbos:0.22.0"
  },
  "dependencies": {
    "@cerbos/grpc": "^0.8.1",
    "@picocss/pico": "^1.5.6",
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
